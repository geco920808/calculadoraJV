<web-app>
    <servlet>
        <servlet-name>CalculadoraServlet</servlet-name>
        <servlet-class>com.calculadora.CalculadoraServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CalculadoraServlet</servlet-name>
        <url-pattern>/CalculadoraServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DatosServlet</servlet-name>
        <servlet-class>com.calculadora.DatosServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DatosServlet</servlet-name>
        <url-pattern>/DatosServlet</url-pattern>
    </servlet-mapping>
</web-app>
